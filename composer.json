{
    "name": "laravel-admin-ext/grid-sortable",
    "description": "Sort the grid data by drag and drop rows",
    "type": "library",
    "keywords": ["laravel-admin", "extension", "grid", "sortable"],
    "license": "MIT",
    "authors": [
        {
            "name": "tungnt",
            "email": "tungnt.code@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "tungnt/laravel-admin": "*@dev",
        "spatie/eloquent-sortable": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Tungnt\\Admin\\GridSortable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tungnt\\Admin\\GridSortable\\GridSortableServiceProvider"
            ]
        }
    }
}
